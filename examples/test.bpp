section Main

type OptionI32 = Some(Int32) : None

let unwrap(_ :: &OptionI32)
    : Some(i) -> i
    : None -> 0

let head(_ :: [Int32])
    : [] -> 0 // panic here
    : [a, ..] -> a

let fibonacci(n :: Int32)
    : 0 | 1 -> n
    : ? -> fibonacci(n - 1) + fibonacci(n - 2)

let is_ok(_ :: Atom)
    : #ok -> true
    : ? -> false

